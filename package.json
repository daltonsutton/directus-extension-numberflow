{
	"name": "directus-extension-numberflow-display",
	"description": "A Directus extension that displays a NumberFlow component.",
	"author": {
		"email": "dalton@sutton.io",
		"name": "Dalton Sutton"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/daltonsutton/directus-extension-numberflow-display.git"
	},
	"bugs": {
		"url": "https://github.com/daltonsutton/directus-extension-numberflow-display/issues"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"homepage": "https://github.com/daltonsutton/directus-extension-numberflow-display",
	"icon": "extension",
	"version": "1.0.1",
	"keywords": [
		"numberflow",
		"directus",
		"directus-extension",
		"directus-extension-display"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "display",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0 || ^11.0.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.4",
		"vue": "^3.5.13"
	},
	"dependencies": {
		"@number-flow/vue": "^0.3.2"
	}
}
